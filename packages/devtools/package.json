{
  "name": "@rxjs-insights/devtools",
  "version": "0.4.0-alpha.0",
  "description": "Connector for the RxJS Insights devtools extension.",
  "keywords": [
    "rxjs",
    "debug",
    "debugger",
    "devtools"
  ],
  "homepage": "https://github.com/ksz-ksz/rxjs-insights",
  "repository": {
    "type": "git",
    "url": "https://github.com/ksz-ksz/rxjs-insights"
  },
  "bugs": {
    "url": "https://github.com/ksz-ksz/rxjs-insights/issues"
  },
  "license": "MIT",
  "author": {
    "name": "ksz ksz",
    "url": "https://github.com/ksz-ksz"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "connect/"
  ],
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/esm/index.js",
      "node": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.js"
    },
    "./connect": {
      "browser": "./dist/esm/connect.js",
      "node": "./dist/cjs/connect.js",
      "types": "./dist/types/connect.d.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:clean && npm run build:js && npm run build:ts",
    "build:clean": "rimraf dist",
    "build:js": "node ../../scripts/build-lib.js",
    "build:ts": "tsc -p tsconfig.lib.json --declaration --emitDeclarationOnly --declarationDir dist/types"
  },
  "dependencies": {
    "@rxjs-insights/core": "^0.4.0-alpha.0"
  }
}
